#!/bin/sh
#
#  Make the stupid bacula.def file so that we don't have to do it
#    manually
#
#  Kern Sibbald, June 2007
#

TOPDIR=`(cd ../../..;pwd)`
TOPDIR=${DEPKGS:-${TOPDIR}}
NM=i686-w64-mingw32-nm
 
echo "LIBRARY bacula.dll"
echo "EXPORTS"
echo " "

cd obj32
# remove "static" objects before running
rm -f parse_conf.* res.*

for i in *.o ; do \
   echo "; $i"; \
   ${NM} $i | grep "^[0-9a-f]* T _" | cut -c13- ; \
   echo " "; \
done

DATA="console_command b_plugin_list plugin_bopen plugin_bclose plugin_bwrite plugin_bread plugin_blseek exepath beef version dist_name"

for i in ${DATA}; do \
   echo "$i DATA"; \
done
