# bextract
BEXTOBJS = bextract.o $(SDCORE_OBJS)

# bscan
SCNOBJS = bscan.o  $(SDCORE_OBJS)

# bcopy
COPYOBJS = bcopy.o $(SDCORE_OBJS)

