DEDUP_LOBJS = $(DEDUP_SRCS:.c=.lo)
# ACSLS changer objects
ACSLSAPILIB = $(ACSLS_LIBDIR)/lib/libapi.a
ACSLSIPCLIB = $(ACSLS_LIBDIR)/lib/libipc.a
ACSLSUTLLIB = $(ACSLS_LIBDIR)/lib/libutl.a
ACSLSCLLIB = $(ACSLS_LIBDIR)/lib/libcl.a
